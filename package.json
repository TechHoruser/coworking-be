{
  "name": "coworking-be",
  "version": "1.0.0",
  "description": "Backend for chat",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mortalswat/coworking-be.git"
  },
  "keywords": [
    "chat",
    "coworking"
  ],
  "author": "Francisco Javier Ponce Rosales",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mortalswat/coworking-be/issues"
  },
  "homepage": "https://github.com/mortalswat/coworking-be#readme",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.11.0",
    "@types/express": "^4.17.17",
    "@types/uuid": "^9.0.1",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
