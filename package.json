{
  "name": "coworking-be",
  "version": "1.0.0",
  "description": "Backend for chat",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register -R test/mocha-reporter.ts test/**/*.spec.ts --timeout 3600000",
    "prisma-migrate": "npx prisma migrate dev --name init",
    "clear-db": "del dev.db && npx prisma migrate dev"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mortalswat/coworking-be.git"
  },
  "keywords": [
    "chat",
    "coworking"
  ],
  "author": "Francisco Javier Ponce Rosales",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mortalswat/coworking-be/issues"
  },
  "homepage": "https://github.com/mortalswat/coworking-be#readme",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "chai": "^4.3.7",
    "child_process": "^1.0.2",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}